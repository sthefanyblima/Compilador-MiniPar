programa-miniPar

SEQ:
    declare i : inteiro
    declare j : inteiro
    
    i = 0
    enquanto i < 5 faca:
        j = 0
        enquanto j < 3 faca:
            escreva("i=", i, " j=", j)
            j = j + 1
        fim_enquanto
        i = i + 1
    fim_enquanto
    
    escreva("Fim do programa")

